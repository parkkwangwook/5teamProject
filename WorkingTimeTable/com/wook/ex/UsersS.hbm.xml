<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wook.ex">

	<class name="Userss" table="Users">

		<id name="userId" type="java.lang.String" column="user_id">
			<generator class="assigned" />
		</id>

		<property name="password" type="java.lang.String"
			column="password" not-null="true" />

		<property name="password2" type="java.lang.String"
			column="password2" not-null="true" />

		<property name="userName" type="java.lang.String"
			column="user_name" not-null="true" />

		<property name="tel" type="java.lang.String"
			column="tel" not-null="false" />

		<property name="email" type="java.lang.String"
			column="email" not-null="true" />

		<property name="birth" type="java.util.Date"
			column="birth" not-null="true" />

		<property name="grade" type="java.lang.String"
			column="grade" not-null="true" />

		<property name="joinDate" type="java.util.Date"
			column="join_date" not-null="true" />

		<property name="question" type="java.lang.String"
			column="question" not-null="true" />

		<property name="answer" type="java.lang.String"
			column="answer" not-null="true" />

	</class>
</hibernate-mapping>
